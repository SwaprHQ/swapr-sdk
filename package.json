{
  "name": "@swapr/sdk",
  "license": "AGPL-3.0-or-later",
  "version": "1.3.0-beta.3",
  "description": "An SDK for building applications on top of Swapr",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "repository": "https://github.com/levelkdev/swapr-sdk.git",
